{
  "name": "react-typed-inputs",
  "version": "1.0.0",
  "description": "Strongly typed input components for React.",
  "keywords": [
    "components",
    "inputs",
    "react",
    "typed"
  ],
  "homepage": "https://github.com/kripod/react-typed-inputs#readme",
  "bugs": {
    "url": "https://github.com/kripod/react-typed-inputs/issues"
  },
  "repository": "github:kripod/react-typed-inputs",
  "license": "MIT",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "pika build",
    "commit": "git-cz",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "publish": "pika publish --no-publish",
    "release": "standard-version",
    "type-check": "tsc --noEmit",
    "version": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "commitizen": "^4.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.5",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.0.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  }
}
